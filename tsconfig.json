{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es7", "es6"],
		"sourceMap": true,
		"outDir": "dist",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true
	}
}
